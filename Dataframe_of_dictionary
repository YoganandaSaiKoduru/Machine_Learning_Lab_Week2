import pandas as pd
data_dict = {
    'Name': ['Alice', 'Bob', 'Charlie', 'David'],
    'Age': [25, 30, 35, 40],
    'City': ['New York', 'San Francisco', 'Los Angeles', 'Chicago']
}

index_labels = ['Person1', 'Person2', 'Person3', 'Person4']
df = pd.DataFrame(data_dict, index=index_labels)
print(df)
